# AWS Amplify Environment Variables Example

# Required for AWS Amplify authentication
AMPLIFY_API_KEY=your_api_key_here
AMPLIFY_USER_POOL_ID=your_user_pool_id_here
AMPLIFY_USER_POOL_CLIENT_ID=your_user_pool_client_id_here

# Required for AWS services
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_REGION=your_aws_region_here
